#!/bin/sh

set -e

if [ "$1" = "install" ] || [ "$1" = "upgrade" ]; then
    for item in /etc/init.d/udev /etc/init.d/udev-finish
    do
        if [ -e "$item" ] && [ -L "$item" ] && [ $(readlink -f "$item") = "/lib/init/upstart-job" ]; then
            rm -f "$item"
        fi
    done
fi

